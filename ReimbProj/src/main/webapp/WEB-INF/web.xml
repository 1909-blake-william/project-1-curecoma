<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>ReimbProj</display-name>
	<context-param>
		<param-name>To</param-name>
		<param-value>Go</param-value>
	</context-param>

	<servlet>
		<servlet-name>Reimbursement</servlet-name>
		<servlet-class>servlets.Login</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ReimbursementMain</servlet-name>
		<servlet-class>servlets.Main</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Reimbursement</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ReimbursementMain</servlet-name>
		<url-pattern>/main</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ReimbursementMain</servlet-name>
		<url-pattern>/main4managers</url-pattern>
	</servlet-mapping>
</web-app>